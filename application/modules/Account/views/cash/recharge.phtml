{%extends file="../../../views/common/page.phtml"%}

{%block name="link"%}
<link rel="stylesheet" type="text/css" href="{%$feroot%}/my/topup/index.css" />
{%/block%}

{%block name="script"%}
<script>
    require(['my/topup/index'], function (main) {
        main.init();
    });
</script>
{%/block%}

{%block name="content"%}
<div class="finish">
    <div class="container finish-bg-color">
        {%include file='../../../views/common/nav.phtml' current="3"%}
        <div class="finish-main">
            <div class="finish-main-title">充值
                <a class="return" href="###">返回</a>
            </div>
            <div class="extract topup">
                <div class="topup-main">
                    <div class="topup-main-select">
                        <div class="topup-main-select-title">选择充值网银</div>
                        <div class="topup-select-content">
                            <div class="topup-select-con-box current zhaoshang">
                                <input type="radio" name="topup-ipt" class="topup-select-ipt" checked="true" data-value="1" />
                            </div>
                            <div class="topup-select-con-box gongshang">
                                <input type="radio" name="topup-ipt" class="topup-select-ipt" data-value="2"/>
                            </div>
                            <div class="topup-select-con-box youzheng">
                                <input type="radio" name="topup-ipt" class="topup-select-ipt" data-value="3"/>
